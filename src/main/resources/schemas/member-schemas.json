 {
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Member",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Full name of the member"
    },
    "email": {
      "type": "string",
      "format": "email",
      "description": "Email address of the member"
    },
    "phoneNumber": {
      "type": "string",
      "pattern": "^[0-9]{10}$",
      "description": "10-digit phone number of the member"
    },
    "membershipDate": {
      "type": "string",
      "format": "date",
      "description": "Date when the membership started"
    }
  },
  "required": ["name", "email", "phoneNumber" , "membershipDate"],
  "additionalProperties": false,

    "examples": [
      {
        "name": "John Doe",
        "email": "john.doe@example.com",
        "phoneNumber": "1234567890",
        "membershipDate": "2025-10-04"
      },
}